my_dict = {'b': 3, 'a': 1, 'c': 2}

# Sort by keys using sorted() function
sorted_dict = {k: my_dict[k] for k in sorted(my_dict)}
print(sorted_dict)  # Output: {'a': 1, 'b': 3, 'c': 2}

# Sort by keys using dict.items() method
sorted_dict = {k: v for k, v in sorted(my_dict.items())}
print(sorted_dict)  # Output: {'a': 1, 'b': 3, 'c': 2}
